typedef struct MF_FRAME_INFO
{
	LONGLONG		llVideoID;			
	M_TIME		mTime;
	M_AV_PROPS	avProps;
	DWORD		cbAudio;
	DWORD		cbVideo;
	LONGLONG		lpVideo;
	LONGLONG		lpAudio;
	LONG			lAudioSamples;
	DWORD		dwProcessID;
